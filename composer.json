{
  "name": "fabio-pv/laravel-simple-bases",
  "description": "A set of base classes to help create projects with Laravel",
  "version": "0.1.0",
  "license": "MIT",
  "require": {
    "php": ">=7.2.0",
    "webpatser/laravel-uuid": "^3.0",
    "spatie/laravel-fractal": "^5.7.0",
    "spatie/laravel-activitylog": "^3.14",
    "barryvdh/laravel-ide-helper": "^2.6",
    "krlove/eloquent-model-generator": "^1.3",
    "mpociot/laravel-apidoc-generator": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "LaravelSimpleBases\\": "src/"
    },
    "files": [
      "src/Helpers/structures.php",
      "src/Helpers/from_to_data.php",
      "src/Helpers/generic.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "fabio\\laravel-simple-bases\\Providers\\LaravelSimpleBaseProvider"
      ]
    }
  }
}
